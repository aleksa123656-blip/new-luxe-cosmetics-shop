{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "app/index.html": {
      "includeFiles": "**/*"
    }
  },
  "env": {
    "PUBLIC_URL": "@public-url"
  }
}